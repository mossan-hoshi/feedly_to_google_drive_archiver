# https://feedly.com/v3/auth/dev
FEEDLY_ACCESS_TOKEN=your_access_token_here
FEEDLY_REFRESH_TOKEN=your_refresh_token_here
FEEDLY_STREAM_ID=user/YOUR_FEEDLY_USER_ID/category/global.all

# Google Drive設定
GOOGLE_DRIVE_FOLDER_ID=YOUR_GOOGLE_DRIVE_FOLDER_ID

# Google Service Account（ローカルテスト用）
# サービスアカウントJSONキーファイルのパス
GOOGLE_SERVICE_ACCOUNT_FILE=path/to/your/service-account-key.json

# GCP設定
GCP_PROJECT_ID=YOUR_PROJECT_ID

# アプリケーション設定
FETCH_PERIOD_DAYS=7